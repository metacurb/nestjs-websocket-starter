{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": "..",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "testTimeout": 30000,
  "modulePathIgnorePatterns": [
    "<rootDir>/dist/"
  ],
  "transformIgnorePatterns": [
    "node_modules/(?!(nanoid)/)"
  ],
  "maxWorkers": 1
}